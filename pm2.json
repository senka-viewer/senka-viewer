{
  "apps": [
    {
      "name": "senka-viewer",
      "instances": "max",
      "script": "node_modules/next/dist/bin/next",
      "args": "start",
      "exec_mode": "cluster_mode",
      "out_file": "/var/log/pm2/senka-viewer/out/out.log",
      "error_file": "/var/log/pm2/senka-viewer/error/error.log",
      "merge_logs": true,
      "env": {
        "PORT": 3000
      },
      "env_production": {
        "PORT": 3000
      }
    }
  ]
}
